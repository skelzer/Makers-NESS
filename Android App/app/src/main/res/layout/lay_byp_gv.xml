<?xml version="1.0" encoding="utf-8"?>
<android.support.percent.PercentFrameLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:layout_width="match_parent"
    android:focusable="false"
    android:clickable="false"
android:layout_height="match_parent"
    android:minHeight="@dimen/notification_large_icon_width">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="false"
    android:clickable="false"
    app:layout_aspectRatio="100%">

    <ImageView
        android:focusable="false"
        android:clickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView5"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imico4"
        android:src="@drawable/horiz"
        android:adjustViewBounds="true"
        android:maxHeight="25dp"
        android:maxWidth="25dp"
        android:minHeight="25dp"
        android:minWidth="25dp"
        android:layout_marginTop="20dp"
        android:layout_toStartOf="@+id/imageView4"
        android:layout_alignEnd="@+id/imico4" />

    <TextView
        android:focusable="false"
        android:clickable="false"
        android:text="New Text"
        android:id="@+id/lay_byp_tv"
        android:layout_marginLeft="20dp"
        android:layout_gravity="center_vertical"
        android:textColorHighlight="#000000"
        android:foregroundTint="#000000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="26sp"
        android:textColor="@color/light_font"

        android:fontFamily="sans-serif-light"
        android:foregroundGravity="top"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/imageView3"
        android:textIsSelectable="true" />

    <ImageView
        android:focusable="false"
        android:clickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:layout_alignParentBottom="true"
        android:src="@drawable/franjablanca"
        />

    <ImageView
        android:focusable="false"
        android:clickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imico1"
        android:src="@drawable/ic_caliente_off"
        android:adjustViewBounds="true"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/imico2"
        android:maxHeight="50dp"
        android:maxWidth="50dp"
        android:padding="5dp"
        android:minHeight="50dp"
        android:minWidth="50dp"/>

    <ImageView
        android:focusable="false"
        android:clickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imico2"
        android:adjustViewBounds="true"
        android:src="@drawable/ic_frio_off"
        android:layout_alignTop="@+id/imico1"
        android:layout_toStartOf="@+id/imico3"
        android:maxHeight="50dp"
        android:maxWidth="50dp"
        android:padding="5dp"
        android:minHeight="50dp"
        android:minWidth="50dp"/>

    <ImageView
        android:focusable="false"
        android:clickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imico3"
        android:adjustViewBounds="true"
        android:src="@drawable/ic_recirc_off"
        android:layout_alignTop="@+id/imico2"
        android:layout_toStartOf="@+id/imico4"
        android:maxHeight="50dp"
        android:maxWidth="50dp"
        android:padding="5dp"
        android:minHeight="50dp"
        android:minWidth="50dp"/>

    <ImageView
        android:focusable="false"
        android:clickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imico4"
        android:adjustViewBounds="true"
        android:src="@drawable/ic_error_off"
        android:layout_alignTop="@+id/imico3"
        android:layout_marginRight="25dp"
        android:layout_alignParentEnd="true"
        android:maxHeight="50dp"
        android:maxWidth="50dp"
        android:padding="5dp"
        android:minHeight="50dp"
        android:minWidth="50dp"/>


    <ImageView
        android:focusable="false"
        android:clickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView4"
        android:src="@drawable/ic_alarm"
        android:adjustViewBounds="true"
        android:maxHeight="50dp"
        android:maxWidth="50dp"
        android:minHeight="50dp"
        android:minWidth="50dp"
        android:padding="10dp"
        android:layout_marginBottom="5dp"
        android:foregroundGravity="top"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/imico3" />


</RelativeLayout>
</android.support.percent.PercentFrameLayout>
